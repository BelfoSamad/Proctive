<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="states">
        <attr name="backgroundType" format="enum">
            <enum name="green" value="0"/>
            <enum name="yellow" value="1"/>
            <enum name="orange" value="2"/>
            <enum name="blue" value="3"/>
            <enum name="red" value="4"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="LabeledInputView">
        <attr name="labelIcon" format="reference" />
        <attr name="labelText" format="string" />
        <attr name="hint" format="string" />
        <attr name="inputStyle" format="reference" />
        <attr name="inputBackgroundColor" format="reference" />
        <attr name="inputType" format="enum">
            <enum name="text" value="0"/>
            <enum name="number" value="1"/>
            <enum name="textarea" value="2"/>
            <enum name="addable" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="OptionsView">
        <attr name="optionBackgroundColor" format="reference" />
        <attr name="optionForegroundColor" format="reference" />
    </declare-styleable>
</resources>